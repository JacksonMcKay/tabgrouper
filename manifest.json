{
    "manifest_version": 3,
    "name": "Tab Grouper",
    "version": "0.0.1",

    "description": "Automatically manage your tab groups in chrome based on the websites you visit and rules you define",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "background": {
        "service_worker": "tabgroups.js"
    },

    "options_page": "options.html",

    "permissions": [
        "tabGroups",
        "storage",
        "tabs"
    ]
}
